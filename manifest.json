{
    "display_information": {
        "name": "Debrief"
    },
    "features": {
        "bot_user": {
            "display_name": "Debrief",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/feedbackuser",
                "description": "Start the feedback loop",
                "usage_hint": "feedback",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "app_mentions:read",
                "commands"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}